{
    "name": "oussema/hide-by-countries",
    "description": "Hide Content Elements depends on the frontend user country",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "countries"
    ],
    "authors": [
        {
            "name": "Oussema Harrabi",
            "homepage": "https://oussemaharrbi.tn",
            "role": "Developer"
        }
    ],
    "homepage": "https://extensions.typo3.org/extension/Hide-By-Countries/",
    "support": {
        "issues": "https://github.com/xvarusx/Hide-By-Countries/issues",
        "source": "https://github.com/xvarusx/Hide-By-Countries"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">= 8.1 < 8.5",
        "typo3/cms-backend": "^13.1",
        "typo3/cms-core": "^13.1",
        "typo3/cms-extbase": "^13.1",
        "typo3/cms-fluid": "^13.1",
        "typo3/cms-frontend": "^13.1",
        "typo3/cms-install": "^13.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.47.1",
        "phpstan/phpdoc-parser": "^1.12.10",
        "phpunit/phpunit": "^10.5.58",
        "typo3/coding-standards": "^0.5.5",
        "typo3/testing-framework": "^8.2.7",
        "webmozart/assert": "^1.11.0"
    },
    "autoload": {
        "psr-4": {
            "Oussema\\HideByCountries\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oussema\\HideByCountries\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "a9f/fractor-extension-installer": true,
            "sbuerk/typo3-cmscomposerinstallers-testingframework-bridge": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "lock": false,
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "hidebycountries",
            "web-dir": ".Build/Web"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ],
        "cs": "php ./.Build/bin/php-cs-fixer fix  --dry-run -v --config ./Build/php-cs-fixer/php-cs-fixer.php ./",
        "csfix": "php ./.Build/bin/php-cs-fixer fix  -v --config ./Build/php-cs-fixer/php-cs-fixer.php ./"
    }
}
